{
  "name": "amanda-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^7.2.1",
    "node-sass": "^7.0.0",
    "pure-react-carousel": "^1.25.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "react-typed": "^1.2.0",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.1",
    "@types/jest": "^25.1.3",
    "react-app-rewired": "^2.1.5"
  },
  "jest": {
    "testMatch": [
      "../**/__tests__/**/*.test.js"
